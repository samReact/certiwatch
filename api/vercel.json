{
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://certiwatch-psi.vercel.app"
        }
      ]
    },
    {
      "source": "(.*)",
      "has": [
        {
          "type": "query",
          "key": "OPTIONS"
        }
      ],
      "headers": [
        {
          "key": "HTTP/1.1",
          "value": "200 ok"
        }
      ]
    }
  ]
}
